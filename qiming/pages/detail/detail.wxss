/**index.wxss**/
page{
  background-color: #f5f5f5;
}
.container {
  padding:0;
  margin: 40rpx 0;
}
.title-icon {
  padding: 4px 3px;
  font-size: 13px;
  text-align: center;
  line-height: 10px;
  margin-left: 5px;
  text-indent: 0;
  color: #65b12c;
  border: 1px solid #d7efc6;
  display: inline-block;
  text-decoration: none;
  vertical-align: middle;
  height: 10px;
  font-style: normal;
  font-weight: 400;
  overflow: hidden;
  box-sizing: content-box;
}
.blank {
  width: 100%;
  height: 10px;
  background-color: #e6e6e6;
}
